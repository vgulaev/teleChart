  yForPie(y) {
    let [a, b] = this.getABfromScroll();
    let r = {};
    let s = {};
    let t = 0;
    for (let e of this.allItems) {
      s[e] = 0;
      for (let i = a; i <= b; i++) {
        s[e] += y[e][i];
      }
      s[e] *= this.data.factor[e];
      t += s[e];
    }
    for (let e of this.allItems) {
      r[e] = s[e] / t * 100;
    }
    return r;
  }
