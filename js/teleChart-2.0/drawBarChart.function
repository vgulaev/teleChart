  drawBarChart(a, b, mm, s) {
    let dx = 'h' + (this.width / (b - a + 1)).toFixed(2);
    let sy = (s.height - 2 * s.yb) / mm.max;
    let y = Math.floor(s.height - this.data.y['y0'][a] * sy);
    let dy = 0, yy = s.yb;
    let d = `M0,${y}` + dx
    for (let i = a + 1; i <= b; i++) {
      yy = Math.floor(s.height - this.data.y['y0'][i] * sy);
      dy = yy - y;
      d += (`v${dy}` + dx);
      y = yy;
    }
    TC20.setA(s['y0'], {d: d});
  }
