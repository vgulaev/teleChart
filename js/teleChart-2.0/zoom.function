  zoom(data) {
    this.zoomMode = true;
    this.cache = {
      XAxis: {
        sieve: this.XAxis.sieve
      },
      scrollBox: {
        x: this.panel.scrollBox.x,
        width: this.panel.scrollBox.width
      }
    };

    // let t = Object.ass
    this.prepareData(data);
    if (1 == this.allItems.size) this.initPathForGraphAndPanel();
    this.XAxis.sieve = 0;

    requestAnimationFrame(() => this.requestZoomAnimation());
  }
