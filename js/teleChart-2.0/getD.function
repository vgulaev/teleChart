  getD(x0, y0, dx, dy, h, minY, maxY, d, a, b) {
    let scaleX = dx / (b - a - 1);
    let scaleY = dy / (maxY - minY);
    let x = x0;
    let y = h - y0 - (d[a] - minY) * scaleY;
    let res = `M${x},${y} `;
    for (let i = a + 1; i < b; i++){
      x = Math.floor(x0 + scaleX * (i - a));
      y = Math.floor(h - y0 - (d[i] - minY) * scaleY);
      res += `L${x},${y} `
    }
    console.log(b, this.data.x.length);
    return res;
  }
