  httpGetAsync(theUrl) {
    return new Promise((resolve, reject) => {
      var xmlHttp = new XMLHttpRequest();
      xmlHttp.onreadystatechange = function() {
        if (xmlHttp.readyState == 4 && xmlHttp.status == 200) resolve(xmlHttp.responseText);
      };
      xmlHttp.open("GET", theUrl, true);
      xmlHttp.send(null);
    });
  }
